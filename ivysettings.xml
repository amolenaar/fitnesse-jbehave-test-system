<ivysettings>
    <settings defaultResolver="default" />
    <include url="${ivy.default.settings.dir}/ivysettings-local.xml"/>
    <resolvers>
    <chain name="default">
        <resolver ref="local"/>
        <ibiblio name="maven2" root="http://repo2.maven.org/maven2" m2compatible="true" />
        <ibiblio name="maven2-snapshots" root="https://oss.sonatype.org/content/repositories/snapshots/" m2compatible="true"/>
    </chain>
    </resolvers>
</ivysettings>
